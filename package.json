    "postcss-svgo": "^4.0.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "prop-types": "^15.7.2",
    "react-scripts": "3.4.1",
    "source-map-explorer": "^2.4.2",
    "tailwindcss": "^1.4.6",
    "tailwindcss-aspect-ratio": "^3.0.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:tailwind": "postcss src/assets/css/style.css -o src/assets/css/style.output.css",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
